<script>
    jQuery(document).ready(function ($) {
        if (document.body.clientWidth > 979) {
            $("#slider_new").attr('style', 'height:' + ($('#d3').height()) + 'px;');
        } else if (document.body.clientWidth > 757) {
            $("#slider_new").attr('style', 'height:' + ($('#d2').height()) + 'px;');
        } else {
            $("#slider_new").attr('style', 'height:' + ($('#d1').height()) + 'px;');
        }
        $("#slider_new").after($("#main_hotspot_zone1"));
        $('.big_label').attr('title', '');
        $('.big_label').click(function (e) {
            e.preventDefault();
        })
    });
    $(window).load(function () {
        $("section#slider_new").attr("style", "display:block");
        $('.crsl-items').carousel({
            speed: 1300,
            autoRotate: 8000
        });
    })
</script>
