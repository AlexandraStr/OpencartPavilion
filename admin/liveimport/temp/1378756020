<script>
    $(document).ready(function () {
        function createCelebSliderHtml(type){
            var celeb_slider_woman = ['4.jpg', '2.jpg', '3.jpg', '1.jpg', '5.jpg', '6.jpg', '7.jpg', '8.jpg', '9.jpg', '10.jpg'];
            var celeb_slider_man = ['1.jpg', '2.jpg', '3.jpg', '4.jpg', '5.jpg', '6.jpg', '7.jpg', '8.jpg', '9.jpg'];
            var result = '';
            if (type == 'woman'){
                list = celeb_slider_woman;
                local = '/data/include/cms/slider_celeb/woman/';
            }else{
                list = celeb_slider_man;
                local = '/data/include/cms/slider_celeb/man/';
            }
            for (pic in list) {
                result += '<div class="crs-celebs-item"><div><img src="' + local + list[pic] + '"></div></div>';
            }
            return result;
        };
        $('#crs-slider-celebs').append(createCelebSliderHtml('man'));
    });
</script>

<!-- przesuwanie slidera celebrities na mobile -->
<script>
    $(document).ready(function () {
        (function ($) {
            $.fn.tactile = function (swipe) {
                return this.each(function () {
                    var $this = $('#ext'),
                            isTouching = false,
                            debut;

                    $this.on('touchstart', debutGeste);

                    function debutGeste() {
                        if (event.touches.length == 1) {
                            debut = event.touches[0].pageX;
                            isTouching = true;
                            $this.on('touchmove', geste);
                        }
                    }

                    function finGeste() {
                        $this.off('touchmove');
                        isTouching = false;
                        debut = null;
                    }

                    function geste() {
                        if (isTouching) {
                            var actuel = event.touches[0].pageX,
                                    delta = debut - actuel;

                            if (Math.abs(delta) >= 30) {
                                if (delta > 0) {
                                    swipe.left();
                                } else {
                                    swipe.right();
                                }
                                finGeste();
                            }
                        }
                        event.preventDefault();
                    }
                });
            };
        })(jQuery);

        /* wyliczanie wielkosci elementow slidera celebryckiego */
        function calcSliderCelebSize() {
            size_dict_slider = {0: 4, 0.01: 4, 0.02: 2, 0.03: 1};
            el_num_page = size_dict_slider[$('#real_size_indicator').css('opacity')];

            /*for Safari 10*/
            if (!el_num_page) {
                $('#ext').css('max-width', $('.big_label.super_margin').width() + 'px');
                var window_width = $(window).width();
                if (window_width < 757) {
                    el_num_page = 1;
                } else if (window_width < 992) {
                    el_num_page = 2;
                } else {
                    el_num_page = 4;
                }
            }
            /*end for Safari 10*/

            ext_width = $('#ext').width();
            product_nums = $('.crs-celebs-item').length;
            el_width = ext_width / el_num_page;
            $('.crs-celebs-item').css('width', el_width + 'px');
            slider_width = el_width * product_nums;
            $('#crs-slider-celebs').css('width', slider_width + 'px');
            //align
            current_index = $('#crs-slider-celebs').attr('mvdc');
            if (-current_index * el_width < -el_width * (product_nums - el_num_page)) {
                $("#crs-slider-celebs").css('margin-left', -el_width * (product_nums - el_num_page) + 'px');
            } else {
                $("#crs-slider-celebs").css('margin-left', -current_index * el_width);
            }
        }
        /* przesuwanie w lewo slider celebrycki*/
        function goleftceleb() {
            $('#crs-celebs-arrow-left').off('click');
            move_by = parseFloat($('.crs-celebs-item').css('width'));
            if (parseFloat($("#crs-slider-celebs").css("margin-left")) <= -move_by) {
                $("#crs-slider-celebs").animate({"margin-left": parseFloat($("#crs-slider-celebs").css("margin-left")) + move_by + "px"});
                $('#crs-slider-celebs').attr('mvdc', parseFloat($('#crs-slider-celebs').attr('mvdc')) - 1);
            } else {
                $("#crs-slider-celebs").animate({"margin-left": 0 + "px"});
            }
            setTimeout(function () {
                $('#crs-celebs-arrow-left').click(function () {
                    goleftceleb()
                })
            }, 400);
        }
        /* przesuwanie w prawo slider celebrycki*/
        function gorightceleb() {
            $('#crs-celebs-arrow-right').off('click');
            move_by = parseFloat($('.crs-celebs-item').css('width'));
            if (parseFloat($("#crs-slider-celebs").css("margin-left")) >= move_by - (parseFloat($("#crs-slider-celebs").css("width"))) + (parseFloat($("#ext").css("width")))) {
                $("#crs-slider-celebs").animate({"margin-left": parseFloat($("#crs-slider-celebs").css("margin-left")) - move_by + "px"});
                $('#crs-slider-celebs').attr('mvdc', parseFloat($('#crs-slider-celebs').attr('mvdc')) + 1);
            } else {
                $("#crs-slider-celebs").animate({"margin-left": parseFloat($("#ext").css("width")) - parseFloat($("#crs-slider-celebs").css("width")) + "px"});
            }
            setTimeout(function () {
                $('#crs-celebs-arrow-right').click(function () {
                    gorightceleb()
                })
            }, 400);
        }

        setTimeout(function () {
            calcSliderCelebSize()
        }, 750);

        $("#crs-celebs-arrow-left").click(function () {
            goleftceleb();
        });

        $("#crs-celebs-arrow-right").click(function () {
            gorightceleb();
        });

        $('#crs-slider-celebs').tactile({
            right: function () {
                goleftceleb();
            },
            left: function () {
                gorightceleb();
            }
        });

        var resizeTimer;
        $(window).resize(function () {
            clearTimeout(resizeTimer);
            resizeTimer = setTimeout(function () {
                calcSliderCelebSize()
            }, 250);
        });
    });

</script>


