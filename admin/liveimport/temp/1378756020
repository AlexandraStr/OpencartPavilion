        /* przesuwanie wyszukiwarki w góry, gdy przykrywa elementy menu */
        function move_search_if_covers_menu() {
            var annoying_element = -$('#menu_categories2>.level_1').outerWidth();
            var menu_width = $('#menu_categories2').width();
            $('#menu_categories2>ul.dl-menu>li').each(function () {
                annoying_element += $(this).outerWidth();
            });
            annoying_element += $('#menu_search_text').outerWidth();
            if (annoying_element > menu_width) {
                $('#menu_search').addClass('move_menu');
            } else {
                $('#menu_search').removeClass('move_menu');
            }
        }
        move_search_if_covers_menu();

        var resizeTimerMenu;
        $(window).resize(function () {
            clearTimeout(resizeTimerMenu);
            resizeTimer = setTimeout(function () {
                move_search_if_covers_menu();
            }, 250);
        });

</script>

