<script type="text/javascript">
    $(document).ready(function () {
        $('.pozycjonerski').before($('.wyroznienia'));
    });

    (function () {

        var quotes = $(".rotate");
        var quoteIndex = -1;

        function showNextQuote() {
            ++quoteIndex;
            quotes.eq(quoteIndex % quotes.length)
                .fadeIn(500)
                .delay(10000)
                .fadeOut(500, showNextQuote);
        }

        showNextQuote();

    })();
</script>
